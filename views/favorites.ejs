<!-- Favorite pokemon will appear on this page -->
<h2> Favorite Pokemon</h2>
<% pokemon.forEach(function(p) { %>
      <a href = "/pokemon/<%= p.name %> " ><h2><%= p.name %></h2> </a>

      <form method="POST" action="/pokemon">
        <input hidden type="text" name="name" value="<%= p.name %>">
      </form>
  <% }); %>

